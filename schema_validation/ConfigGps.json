{
  "title": "ConfigGps",
  "type": "object",
  "properties": {
    "baudRate": {
      "description": "baud rate value",
      "type": "integer"
    },
    "connection": {
      "description": "connection type",
      "type": "string",
      "enum": ["BT", "RS232"]
    },
    "configString": {
      "description": "ASCII string to send to GPS",
      "type": "string"
    },
    "enable": {
      "description": "enable the GPS [true|false]",
      "type": "boolean"
    },
    "type": {
      "description": "gps type",
      "type": "string",
      "enum": ["internal", "NMEA", "Trimble"]
    },
    "timestamp": {
      "description": "timestamp in iso8601 format, example: 2018-08-01T12:58:40.877Z",
      "type": "string"
    },
    "uuid": {
      "description": "Unique message id for tracking config lifecycle",
      "type": "string"
    }
  },
  "additionalProperties": false
}
